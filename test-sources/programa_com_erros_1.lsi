def funcSub(int A, int B) {
    int Diff;
    Diff := A - B;
    return Diff;; # Dois pontos e vírgula
}

def funcDiv(int A, int B)) { # ")" sem sua respectiva "("
    int Quotient;
    if (B != 0) {
        Quotient := A / B;
    } else {
        Quotient := 0;  # Evita divisão por zero
    }
    return Quotient;
}

def principal() {
    int X, Y, R;
    X := 10;
    Y := 3;

    # Chamadas de função e aritmética
    R := funcSub(X, Y);  # R = 7 (10 - 3)
    R := R + 2;          # R = 9
    R := R * Y;          # R = 27 (9 * 3)

    # Condicional simples com retorno sem valor
    if (X > Y) {
        print X;         # imprime 10
        return;          # retorna sem valor
    }

    # Se chegar aqui, significa que X <= Y
    if (X == Y) {
        print R;
    }
    else {
        R := funcDiv(X, Y);  # caso X != Y e X <= Y, R será X / Y
        print R;
    }

    # Outro teste condicional com operador !=
    if (X !!= Y) {
        Temp := funcMod(X, Y);  # Erro: funcMod não está definida
        print Temp;       # resultado da função funcMod
    }

    # Impressão final
    print R;

    return;
}